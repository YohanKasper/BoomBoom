<%= simple_form_for(@offer) do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :date %>
  <%= f.input :address %>
  <%= f.input :photo, as: :file %>
  <%= f.input :category, collection: ['Lights', 'Speakers', 'Food Trucks', 'Drinks', 'Costumes', 'Others'], label: 'Choose your category' %>
  <%= f.submit %>
<% end %>
